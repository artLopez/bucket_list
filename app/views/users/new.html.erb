<h1>New Account</h1>

<%= form_for :users, url: users_path do |f| %>
    <p>
      <%= f.label :firstName %><br>
      <%= f.text_field :firstName  %>
    </p>

    <p>
      <%= f.label :lastName %>
      <%= f.text_field :lastName %>
    </p>

    <p>
      <%= f.label :username %>
      <%= f.text_field :username%>
    </p>

    <p>
      <%= f.label :password %><br>
      <%= f.password_field :password %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>
